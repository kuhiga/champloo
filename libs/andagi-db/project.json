{
  "name": "andagi-db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/andagi-db",
  "projectType": "library",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx supabase start",
        "cwd": "libs/andagi-db"
      }
    },
    "stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx supabase stopn",
        "cwd": "libs/andagi-db"
      }
    },
    "test": {
      "executor": "@nx/deno:test",
      "outputs": ["{workspaceRoot}/coverage/libs/andagi-db"],
      "options": {
        "coverageDirectory": "coverage/libs/andagi-db",
        "denoConfig": "libs/andagi-db/deno.json",
        "check": "local"
      }
    },
    "lint": {
      "executor": "@nx/deno:lint",
      "options": {
        "denoConfig": "libs/andagi-db/deno.json"
      }
    }
  },
  "tags": []
}
